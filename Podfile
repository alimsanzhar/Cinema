target 'Cinema' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Cinema
  pod 'ObjectMapper', '~> 3.3'
  pod 'Alamofire'
  pod 'Cartography', '~> 3.0'
  pod 'RealmSwift'
  pod 'SwiftLint'
  pod 'Kingfisher'
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxDataSources'

  target 'CinemaTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'CinemaUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
